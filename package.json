  "license": "BSD-2-Clause",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-jsy": "^0.8.1",
    "babel-preset-react": "^6.24.1",
    "glamor": "^2.20.40",
    "glamorous": "^4.9.1",
    "http-server": "^0.10.0",
    "lodash": "^4.17.4",
    "nodemon": "*",
    "npm-run-all": "^4.1.1",
    "pify": "^3.0.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "rollup": "0.49.2",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-progress": "^0.4.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-plugin-visualizer": "^0.3.1",
    "semantic-ui-css-latolatin": "^2.2.13",
    "semantic-ui-react": "^0.73.0",
    "uglify-es": "^3.1.0"
  },
  "babel": {
    "presets": "jsy/lean"
  },
  "scripts": {
    "start": "node ./dist/server.js",
    "start:static": "http-server -c-1 ./public",
    "build": "run-p build:*",
    "build:css": "cp -R ./node_modules/semantic-ui-css-latolatin/dist/* ./public/css/",
    "build:server": "babel -s inline -x .js,.jsy code -d dist",
    "build:app": "babel-node ./rollup.cfg.jsy",
    "watch": "run-p watch:*",
    "watch:server": "npm -s run build:server -- --watch",
    "watch:app": "npm -s run build:app -- --watch"
  },
  "dependencies": {
    "ec-pem": "^0.16.0",
    "express": "^4.15.4",
    "express-static-gzip": "^0.3.0",
    "spdy": "^3.4.7"
  }
}
